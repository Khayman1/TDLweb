<%- include('./include/header') %>

<!-- Main -->
<main id="site-main">

  <h3>사용자 등록</h3>

  <form action="/register" method="POST" class="register">
    <label for="name"><b>이름</b></label>
    <input type="text" placeholder="이름" name="name" id="name">
    <label for="Email"><b>이메일</b></label>
    <input type="text" placeholder="이메일" name="Email" id="Email">
    <label for="password"><b>비밀번호</b></label>
    <input type="password" placeholder="비밀번호" name="password" id="password">
    <input type="submit" value="등록" class="register-btn">
  </form>    
</main>
<!-- /Main -->

<%- include('./include/footer') %>